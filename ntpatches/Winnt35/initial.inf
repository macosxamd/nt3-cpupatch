                read-syms NonFatalError7$(!STF_LANGUAGE)
                shell "subroutn.inf" SetupMessage $(!STF_LANGUAGE) "NONFATAL" $(NonFatal)
                goto computername
            else-ifstr(i) $(Name) == ".."
                read-syms NonFatalError7$(!STF_LANGUAGE)
                shell "subroutn.inf" SetupMessage $(!STF_LANGUAGE) "NONFATAL" $(NonFatal)
                goto computername
